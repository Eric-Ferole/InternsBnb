<%= form_tag jobs_path, method: :get do %>
  <%= text_field_tag :query,
    params[:query],
    class: "form-control",
      id: "search-query",
    placeholder: "Find an internship!"
  %>
  <div>
    <ul id="autocomplete-ul">
    </ul>
  </div>


